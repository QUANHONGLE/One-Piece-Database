{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/build"
      }
    },
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/cards",
      "destination": "/api/cards"
    },
    {
      "source": "/api/sets",
      "destination": "/api/sets"
    }
  ]
}
