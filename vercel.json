{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/cards",
      "destination": "/api/cards"
    },
    {
      "source": "/api/sets",
      "destination": "/api/sets"
    }
  ]
}
